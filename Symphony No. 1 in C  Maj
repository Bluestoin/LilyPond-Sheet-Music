 f2. b8. d16
 f8 d b g f d b g
  c4 r4 <f c' a'> r4
 <e c' g'> r4 <g, d' b'> r4
 <g e' c'>2. e'4
 g2. f16 e d c 
 b8 <b d> <b d> <b d> <b d>2
 <b d> <b d>
}

violaMusic = \relative { \clef alto 
 c'4\f ^"pizz." r4 f,\p r4 %Page 1
 g\f r4 a\p r4 
 c,\cresc r4 d' d
 g\f ^"arco" r4 r2
 f4\p (d2 g,4)
 g' (e c g)
  
 g4 (f') ~ f16 e (f c d e f d) %Page 2
 c4\f r4 f^"ten."\f r4
 g,2 ~\p g8\cresc g g g
 a4\f r4 <f c' f>^"ten."\f r4
 g4\f r4 r2
 g8\p (a16 b c d e f) g4. g32 (f e d)
 c4 r4 r2
 e,4 r4 r2
 
 e4 r4 e r4 %Page 3
 e r4 r2
 r1
 r2 r4 a16 (g f e)
 d4\p r4 r2
 f4 r4 r2
 f4 r4 f r4
 f4 r4 r2
 d'1(
 \afterGrace c1) { c16 d }
 b2.\sf\sf r4
 
 r2 r4 g8. b16 %Page 4
 d2. r4
 r2 r4 b8. d16
 f2. r4
 r2 f8 d b g
 c4 r4 f r4
 g r4 g, r4
 c8 <e, g> <e g> <e g> <e g>2
 <e g> <e g>
 <f g> <f g>
 <f g> <f g>
}

