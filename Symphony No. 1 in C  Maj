violinIMusic = \relative { 
 <g' b e>4\f ^"pizz." r4 <f a f'>\p r4 %Page 1
 <g, f' b>\f r4 <a e' c'>\p r4
 <d c' f>\cresc r4 <d c' f> <d c' f>
 <g d' b' f>4.\f^"arco"
 gis'8 (a f! d c)
 b8.\p (d32 c) b4 ~ b16 c (d e f d c b)
 d8. (c16) c4 ~ c16 b (c b c b c cis)
 
 d8. (f32 e) d4 ~ d16 (c d e f g a f) %Page 2
 e4\f r4 <a,, f' d'>^"ten."\f r4
 <g e' c'>4.\p e''8 e\cresc (d c b)
 <a, e' c'>4\f r4 <f' c' a'>^"ten."\f r4
 <g, e' c'>4\f r4 r2
 g8\p (a16 b c d e f) g4. g32 (f e d)
 c2. g8. b16
 c2. g8. b16
 
 c8-. c-. g-. b-. c-. c-. g-. b-. %Page 3
 c4-. e-. g-. b-.-.
 c r4 r2
 r2 r4
 a16 (g f e)
 d2.\p a8. (cis16)
 d2. a8. (cis16)
 d8 d-. a-. cis-. d-. d-. a-. c-.
 d4-. f-. a-. cis-.
 d r4 r2
 \afterGrace c,1 { c16 (d) }
 d2.\sf g8. b16 
 
 d,8 d g, b d d g,8. b16 %Page 4
 d2. g,8. b16
 d8 d g, b d d b8. d16
 f2. b8. d16
 f8 d b g f d b g
 c4 r4 <f c' a'> r4
 <e c' g'> r4 <g, d' b'> r4
 <g e' c'>2. e''4
 g2. f16 e d c 
 <d, b'>2 <d b'>
 <d b'> <d b'>

}

violinIIMusic = \relative { 
 <g e'>4\f ^"pizz." r4 <a f'>\p r4 %Page 1
 <g f' b>\f r4 <a e' c'>\p r4
 <d c'>\cresc r4 <d c'> <d c'>
 <g, d' b'>\f^"arco" r8 gis'8 (a f! d c)
 b8.\p (d32 c) b4 ~ b16 c (d e f d c b)
 d8. (c16) c4 ~ c16 d (c d c d c cis)
 
 d8. (f32 e) d4 ~ d16 c (d e f g a f) %Page 2
 <g, e'>4\f r4 <a f' d'>^"ten."\f r4
 <g e'>4.\p e'8 e\cresc (d c b)
 <c e a>4\f r4 <f c' a'>^"ten."\f r4
 <g, e' c'>4\f r4 r2
 g8\p (a16 b c d e f) g4. g32 (f e d)
 c4 r4 r2
 g4 r4 r2
 
 g4 r4 g r4 %Page 3
 g4 r4 r2
 r1
 r2 r4 a'16 (g f e)
 d4\p r4 r2
 a4 r4 r2
 a4 r4 a r4
 a4 r4 r2
 d1(
 \afterGrace c1) { c16 (d) }
 b2.\sf r4

 r2 r4 g8. b16 %Page 4
 d2. r4
