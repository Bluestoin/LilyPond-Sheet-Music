\score {
  <<
  \new StaffGroup = "StaffGroup_woodwinds" <<
    \new Staff = "Staff_flute" \with { instrumentName = "Flauto"}
    \fluteMusic   
    \new Staff = "Staff_oboe" \with { instrumentName = "Oboe" }
    \oboeMusic   
    \new Staff = "Staff_clarinet" \with { instrumentName = "Clarinetto in C" }
    \clarinetMusic    
    \new Staff = "Staff_bassoon" \with { instrumentName = "Fagotto" }
    \bassoonMusic 
  >>
  
  \new StaffGroup = "StaffGroup_brass" <<
    \new Staff = "Staff_horn" \with { instrumentName = "Corno in C" } 
    \hornMusic   
    \new Staff = "Staff_trumpet" \with { instrumentName = "Tromba in C" } 
    \trumpetMusic
  >>
  
  \new Staff = "Staff_timpani" \with { instrumentName = "Timpani in C G" } 
  \timpaniMusic
    
  \new StaffGroup = "StaffGroup_strings" <<
    \new GrandStaff = "GrandStaff_violins" <<
    \new Staff = "Staff_violinI" \with { instrumentName = "Violino I" }
    \violinIMusic
    \new Staff = "Staff_violinII" \with { instrumentName = "Violino II" } 
    \violinIIMusic    
    >>
    \new Staff = "Staff_viola" \with { instrumentName = "Viola" } 
    \violaMusic
    \new Staff = "Staff_violoncello" \with { instrumentName = "Violoncello" } 
    \violoncelloMusic
    \new Staff = "Staff_doublebass" \with { instrumentName = "Contrabasso" } 
    \doublebassMusic
    
  >>
  >>
  
}
