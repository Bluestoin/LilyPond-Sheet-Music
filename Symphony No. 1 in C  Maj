\version "2.22.2"
\language "nederlands"

\header {
  dedication = "Baron Gottfried Freiherr van Swieten"
  title = "Symphony No. 1"
  subtitle = " in C Major"
  composer = "Ludwig van Beethoven"
  opus = "Op. 21"
}

\paper {
  #(set-paper-size "a4")
}

#(set-global-staff-size 11.22)

fluteMusic = \relative { 
  \tempo "Adagio molto" 8. = 88 %Page 1
  <bes'' e>2\fp _(^( <a f'>8)) r8 r4 
  <b! f>2\fp _(^( <c e,>8)) r8 r4
  <fis c>2\cresc <fis c>4^. ( <fis c>^.)
  <b, g'>\f r4 r2
  r4 f\p (d b)
  r4 g' (e c) \break

  <<
   \new Voice = "first" %Page 2
   \relative { \voiceOne  d''2\cresc ~ d16 (c d e f g a f) 
   <c e>4\f \stemDown <e' g>^"ten."\f r4 <d f>^"ten."\f
   <c e>4.\p (<e g>8) <e g>\cresc (<d f> <c e> <b d>)
   c4\f  <c e>^"ten."\f r4 <d f>4^"ten."\f}
   \new Voice= "second"
   \relative { \voiceTwo  r2 r4 d''16 (e f d)
   s1
   s1
   c'4}
 >>
 <<
   \new Voice = "first"
   \relative { \voiceOne  }
   \new Voice= "second"
   \relative { \voiceTwo  }
 >>
 <c e>2\f <c e>4 (<c e>)
 <b f'>2\p (<f b>)
 \time 2/2 
 \bar ".|:" \tempo "Allegro con brio" 2 = 112
 <e c>4 r4 r2 
 r1 
 
 r1 %Page 3
 r1
 <e' c'>1\<
 _(^(<e c'>))
 <f d'>4\p r4 r2
 r1
 r1
 r1
 <f d>1\<
 _(^(<f d>))
 <f d>2.\sf r4 \break
 
 r1 %Page 4
 b2. r4
 r1
 <b d>
 <b d>4 <b d> <b d> <b d>
 <c e> r4 <a f'> r4
 <g e'> r4 <f b> r4
 <e c'> r4 r2
 r4 c'4 c c
 b2. d4 \break
 
 f2. e16 d c b %Page 5
 c4 r4 r2 
 r4 c c c
 b8 a g a b c d e
 f e f e f d c b
 c4 r2 a8 c
 d4 r2 b d
}

oboeMusic = \relative { 
 <bes' e>2\fp _(^( <a f'>8)) r8 r4 %Page 1
 <b! f'>2\fp _(^( <c e>8)) r8 r4
 <c fis>2\cresc <c fis>4^.( <c fis>^.)
 <b g'>\f r4 r2
 g'1 ~\p
 g4 ~ g16 fis g fis g8-. (g-. g-. g-.)

 g2. <d f>16 (<e g> <f a> <d f>) %Page 2
 <c e>4\f <e g>^"ten."\f r4 <d f>^"ten."\f
 <c e>4.\p (<e g>8) <e g>\cresc (<d f> <c e> <b d>)
  <<
   \new Voice = "first"
   \relative { \voiceOne c''4 <c e>^"ten."\f r4 <d f>4^"ten."\f }
   \new Voice= "second"
   \relative { \voiceTwo c''4\f }
 >>
 <c e>2\f <c e>4 (<c e>)
 <b f'>2\p (<f b>) 
 <e c>4 r4 r2 
 r1
 
 r1 %Page 3
 r1
 <<
   \new Voice = "first"
   \relative { \voiceOne 
   e''1
   (g)
   }
   \new Voice= "second"
   \relative { \voiceTwo 
   s1
   e''2. (a,4) }
 >>
 <a f'>4\p r4 r2 
 r1
 r1
 r1
 <f' a>1\<
 _(^(<f a>))
 <f g>2.\sf r4
 
 r1 %Page 4
 <f g>2. r4
 r1
 <g b>
 <g b>4 <g b> <g b> <g b>
 <g c> r4 <c, a'> r4
 <c e> r4 <b d> r4
 <e, c'> r4 r2
 r4 <c' e> <c e> <c e>
 <b f'>1 
 
 <b f'>4 <f b> <f b> <f b> %Page 5
 <e c> r4 r2
 r4 <c e> <c e> <c e>
 <b f>1
 <b f>4 <f b> <f b> <f b>
 <e c> r2 a8 c
 d4 r2 b8 d
 
}

clarinetMusic = \relative { 
  \transposition bes
  <g' b >2\fp _(^( <f a>8)) r8 r4 %Page 1
  <d f>2\fp _(^( <c e>8)) r8 r4
  <a' c>2\cresc <a c>4-.( <a c>-.)
  <g b>\f r4 r2
  g1 ~\p g4 ~ g16 fis g fis g8-. (g-. g-. g-.)
  
  g2. <d f>16 (<e g> <f a> <d f>) %Page 2
  <c e>4\f <g' e'>^"ten."\f r4 <d f>^"ten."\f
 <<
   \new Voice = "first"
   \relative { \voiceOne <c' e>4.\p (<e g>8) g\cresc (f e f)
   e4\f <c e>^"ten."\f r4 <f a>^"ten."\f }
   \new Voice= "second"
   \relative { \voiceTwo s2 e'8 d c f
   e4}
 >>
 <c e>2\f <e c'>4 (<e c'>)
 <d b'>2\p (<f b>)
 <e c>4 r4 r2 
 r1
 
 r1 %Page 3
 r1
 r1
 <c' e>\<
 _(^(<d f>4\p)) r4 r2
 r1
 r1
 r1
 <f, a>1\<
 _(^(<f a>))
 <f g>2.\sf r4
 
 r1 %Page 4
 <f b>2. r4
 r1
 <b d>1
 <b d>4 <b d> <b d> <b d>
 <c e> r4 <a f> r4
 <g e> r4 <b f> r4
 <e c> r4 r2
 r1
 b2. d4
 f2. e16 d c b
 
}

bassoonMusic =\relative { 
  \clef bass
 <c c'>2\fp _(^( <f, f'>8)) r8 r4 %Page 1
 <g g'>2\fp _(^( <a a'>8)) r8 r4
 <d d'>2\cresc <d d'>4-.( <d d'>4-.)
 <g, g'>4\f r4 r2
 
 <<
   \new Voice = "first"
   \relative { \voiceOne  
    r4 f'4 (d b) %Page 1
    r4 g'4 (e c)
    
    f2 ~ f16 e f c d e f d %Page 2, voice 1
   c4\f <c, c'>^"ten."\f r4 \stemDown <f f'>^"ten."\f
   <g g'>2\p \stemUp g8\cresc g g (b) 
   \stemDown <a c>4\f <a, a'>^"ten."\f r4 \stemUp <f f'>^"ten."\f}
   \new Voice= "second"
   \relative { \voiceTwo g,8\p g' g g f f f f
   e e e e e e e e
   
   b8 b' b b b2 
   c!4 s2 
   s2 s4
   g,8 g' g (gis)
   }
 >>  
  <g g'>2\f <g g'>4 (<g g'>)
  <g g'>2\p <g g'>
  <c c'>4 r4 r2 
  r1
  
  r1
  r1
  <c' g'>1\< %Page 3
  _(^(<a g'>))
  <d, f'>4\p\p r4 r2
  r1
  r1
  r1
  <d d'>1\<
  _(^(<d d'>))
  <b b'>2.\sf r4
  
  r2 r4 <g g'>8. <b b'>16
  <d d'>2. r4
  r2 r4 <b b'>8. <d d'>16
  <f f'>1
  <f f'>4 <f f'> <f f'> <f f'>
  <e e'> r4 f r4 
  g r4 g r4
  <<
   \new Voice = "first"
   \relative { \voiceOne  
   c4 r4 r2
   s1
   b'2. d4
   f2. e16 d c b}
   \new Voice= "second"
   \relative { \voiceTwo 
   c2 c 
   c2 c
   c c
   c c}
 >>


}

hornMusic = \relative { 
  \transposition f
 <c' c'>2\fp _(^( <c c'>8)) r8 r4 %Page 1
 <g' d'>2\fp _(^( <c, c'>8)) r8 r4
 d'2\cresc ( d4-. d-. )
 <g, d'>\f r4 r2
 <g g'>1 ~\p
 <g g'>4 <g g'> (<e e'> <c c'>)

 <g g'>2 ~ <g g'>8 <g g'> <g g'> <g g'> %Page 2
 <c c'>4\f <e' g>^"ten."\f r4 <d f>^"ten."\f
 <c e>4.\p (<e g>8) <e g> (<d f> <c e> <d f>)
 <c e>4\f <c, c'>^"ten."\f r4 <c c'>^"ten."\f
 <c c'>2 ~\f <c c'>8 (<g' g'> <e e'> <c c'>)
